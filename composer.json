{
    "name": "zeroicq/directory-counter",
    "description": "PHP CLI app recursively summing numbers from \"count\" files.",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "zeroicq"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "symfony/console": "^6.2",
        "ext-gmp": "*",
        "symfony/event-dispatcher": "^6.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.16",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0"
    }
}
